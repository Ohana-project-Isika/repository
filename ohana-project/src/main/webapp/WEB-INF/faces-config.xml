<?xml version="1.0"?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/billetterie/details.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{eventController.back}</from-action>
			<from-outcome>billetterie</from-outcome>
			<to-view-id>/billetterie.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/reservation.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{eventController.back}</from-action>
			<from-outcome>billetterie</from-outcome>
			<to-view-id>/billetterie.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginController.validateLogin}</from-action>
			<from-outcome>reservation</from-outcome>
			<to-view-id>/billetterie/reservation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/reservation.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginController.logIn}</from-action>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/reservation.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paymentController.pay}</from-action>
			<from-outcome>verificationPanier</from-outcome>
			<to-view-id>/billetterie/verificationPanier.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/verificationPanier.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{eventController.back}</from-action>
			<from-outcome>billetterie</from-outcome>
			<to-view-id>/billetterie.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/verificationPanier.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paymentController.backToCart}</from-action>
			<from-outcome>reservation</from-outcome>
			<to-view-id>/billetterie/reservation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/verificationPanier.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paymentController.completePayment}</from-action>
			<from-outcome>completePayment</from-outcome>
			<to-view-id>/billetterie/completePayment.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/completePayment.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paymentController.validatePayment}</from-action>
			<from-outcome>validatePayment</from-outcome>
			<to-view-id>/billetterie/validatePayment.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/validatePayment.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{paymentController.download}</from-action>
			<from-outcome>telechargement</from-outcome>
			<to-view-id>/billetterie/telechargement.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie/validatePayment.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{eventController.back}</from-action>
			<from-outcome>billetterie</from-outcome>
			<to-view-id>/billetterie.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/billetterie.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reservation</from-outcome>
			<to-view-id>/billetterie/reservation.xhtml</to-view-id>
			<redirect />
		</navigation-case>

	</navigation-rule>

	<navigation-rule>
		<from-view-id>/association/associationTable.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{membershipController.createMembershipManage(o.idAssos)}</from-action>
			<from-outcome>createMembershipForm</from-outcome>
			<to-view-id>/membership/createMembershipForm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{dashboardAssociationController.ReadListMemberByAssociation(o.idAssos)}</from-action>
			<from-outcome>bar</from-outcome>
			<to-view-id>/bar.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/association/listAssociation.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{associationController.ShowAssociation(o.idAssos)}</from-action>
			<from-outcome>showAssociation</from-outcome>
			<to-view-id>/association/showAssociation.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{membershipController.subMemberForm(o.idAssos)}</from-action>
			<from-outcome>subMember</from-outcome>
			<to-view-id>/membership/subMember.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/membership/createMemberForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{membershipControllercreateMember(membershipController.membership.idMbs)}</from-action>
			<from-outcome>showMember</from-outcome>
			<to-view-id>/membership/showMember.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/membership/subMemberForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{memberController.subMember(membershipController.membership.idMbs, loginController.accountDto.idAccount)}</from-action>
			<from-outcome>showMember</from-outcome>
			<to-view-id>/membership/showMember.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/indexOhana.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginController.logout()}</from-action>
			<from-outcome>indexOhana</from-outcome>
			<to-view-id>/indexOhana.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>indexOhana</from-outcome>
			<to-view-id>/createAccount.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createAccount</from-outcome>
			<to-view-id>/createAccount.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/crowdfunding/listProjectAssos.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{fundingMB.finance(o.id)}</from-action>
			<from-outcome>financeProject</from-outcome>
			<to-view-id>/crowdfunding/financeProject.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/association/createAssociationForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{associationController.subscription(accountController.accountnew.idAccount)}</from-action>
			<from-outcome>ValidateSubscription</from-outcome>
			<to-view-id>/ValidateSubscription.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
</faces-config>
